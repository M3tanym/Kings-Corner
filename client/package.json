{
  "name": "chess-app",
  "version": "2.0.0",
  "description": "A Modern Web-App Based Chess Application",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "startServer": ". ../venv/bin/activate && python3 ../server/manage.py runserver",
    "startElectron": "concurrently \"npm run startServer\" \"webpack-dev-server -d --config ./webpack.electron.config.js\"",
    "startWebsite": "concurrently \"npm run startServer\" \"webpack-dev-server -d --config ./webpack.website.config.js\"",
    "start": "npm run startWebsite",
    "buildElectron": "webpack -p --config ./webpack.electron.config.js && electron-forge publish",
    "buildWebsite": "webpack -p --config ./webpack.website.config.js",
    "build": "npm run buildWebsite && npm run buildElectron"
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.2",
    "autoprefixer": "^10.1.0",
    "chess.js": "^0.11.0",
    "clsx": "latest",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^2.9.5",
    "graphql": "^15.5.0",
    "notistack": "^1.0.2",
    "react": "^17.0.1",
    "react-chessground": "^1.5.0",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.1.25",
    "react-router-dom": "^5.2.0",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.4.1",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "6.0.0-beta.54",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "5.0.1",
    "electron": "11.0.4",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "4.5.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "precss": "^4.0.0",
    "sass-loader": "^10.1.0",
    "style-loader": "2.0.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
