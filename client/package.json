{
  "name": "kings-corner",
  "author": "Meta Games",
  "version": "1.0.0",
  "description": "A Modern Chess Web App",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "startServer": ". ../venv/bin/activate && python3 ../backend/app/app.py",
    "startServerMac": ". ~/.virtualenvs/Kings-Corner/bin/activate && python3 ../backend/app/app.py",
    "startElectron": "webpack serve --mode development --config webpack.electron.config.js",
    "startWebsite": "webpack serve --mode development --config webpack.website.config.js",
    "start": "concurrently --kill-others-on-fail \"npm:startServer\" \"npm:startWebsite\"",
    "buildElectron": "webpack --mode production --config webpack.electron.config.js && electron-forge publish",
    "buildWebsite": "webpack --mode production --config webpack.website.config.js",
    "build": "npm run buildWebsite && npm run buildElectron",
    "deploy": "npm run build && npm run startServer"
  },
  "config": {
    "forge": "./forge.js"
  },
  "dependencies": {
    "@apollo/client": "^3.3.11",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.3",
    "autoprefixer": "^10.2.4",
    "chess.js": "^0.11.0",
    "chessground": "^7.9.3",
    "clsx": "latest",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^3.3.0",
    "graphql": "^15.5.0",
    "notistack": "^1.0.3",
    "npm-check-updates": "^11.1.4",
    "react": "^17.0.1",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dnd-touch-backend": "^11.1.3",
    "react-dom": "^17.0.1",
    "react-google-login": "^5.2.2",
    "react-router-dom": "^5.2.0",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^4.4.1",
    "recharts": "^2.0.6",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-dmg": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@electron-forge/publisher-github": "6.0.0-beta.54",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "5.0.2",
    "electron": "11.2.3",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.1.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "sass": "^1.32.7",
    "sass-loader": "^11.0.1",
    "style-loader": "2.0.0",
    "webpack": "^5.22.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
